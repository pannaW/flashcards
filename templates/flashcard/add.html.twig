{% extends 'layout.html.twig' %}

{% block title %} {{ 'title.add_flashcard'|trans }} {% endblock %}

{% block body %}
<h1> {{ 'title.add_flashcard'|trans }} </h1>
    {{ form_start(form, {method: 'post', action : url('flashcard_add')}) }}
    {{ form_widget(form) }}
    <input type="submit" value="{{ 'action.add'|trans }}" class="btn btn-success" />
    {{ form_end(form) }}
{% endblock %}