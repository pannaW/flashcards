{% extends 'layout.html.twig' %}

{% block title %} {{ 'title.registration'|trans }} {% endblock %}

{% block body %}
    <h1> {{ 'title.registration'|trans }} </h1>
    {{ form_start(form, {method: 'post', action : url('register')}) }}
    {{ form_widget(form) }}
    <input type="submit" value="{{ 'action.registration'|trans }} " class="btn btn-success" />
    {{ form_end(form) }}
{% endblock %}